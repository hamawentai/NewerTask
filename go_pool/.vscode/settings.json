    "editor.fontSize": 14,
    "editor.lineHeight": 23,
    "files.autoSave": "onFocusChange",
    "emmet.excludeLanguages": [
        "markdown"
    ],
    "editor.detectIndentation": true,
    "editor.tabSize": 4,
    // "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "modifications",
    "editor.fontFamily": "JetBrains Mono, 'Courier New', monospace",
    "editor.showUnused": true,
    // google 风格的代码格式化
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 4, ColumnLimit: 120}",
    "C_Cpp.clang_format_style": "{ BasedOnStyle: Google, IndentWidth: 4, ColumnLimit: 120}",
    "C_Cpp.clang_format_sortIncludes": true,
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "C_Cpp.formatting": "Default",
    "C C++ Header": {
        "scope": "c, cpp",
        "prefix": "header",
        "description": "Add #ifndef, #define and #endif",
        "body": [
            "#ifndef _${TM_FILENAME_BASE/(.*)/${1:/upcase}/}_H_",
            "#define _${TM_FILENAME_BASE/(.*)/${1:/upcase}/}_H_",
            "",
            "$0",
            "",
            "#endif // _${TM_FILENAME_BASE/(.*)/${1:/upcase}/}_H_"
        ]
    },
    "workbench.colorCustomizations": {
        "[Atom One Light]": {
